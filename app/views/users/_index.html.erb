<h1>Users</h1>
			<table class="table table-hover">
				<thead>
					<tr>
					<th>image</th>
					<th>name</th>
					<th></th>
					</tr>
				</thead>

				<tbody>
					<tr>
						<% users.each do |user| %>

						<td><%= attachment_image_tag user, :profile_image, size:"60x60", fallback:"no_image.png", format:'png' %></td>
						<td><%= user.name %></td>
						<td><%= link_to "Show", user_path(user.id) %></td>
					</tr>
				</tbody>
				<% end %>
			</table>
